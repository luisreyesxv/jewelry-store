        <SmoothLoadingImage  id=\"success-modal-image\" src=\"https://images.nappy.co/uploads/large/81596240559elidpvwvuisi8su7074mbcw2gk2svuemo3xrzmiryjit7qdugi7pozovzactyb5xdwbab7ijtcx54lpk5gtkrv0lc5yke4jtj5fp.jpg?auto=format&fm=jpg&w=500&q=75\" style={{width: \"20px\"}}/>\n                    <Modal.Description>\n                    Thank you for purchasing from <strong>Bartolome Jewelry</strong>. An email with your receipt has been sent to <i>{props.user}</i>. Please check that for more details.\n\n                    </Modal.Description>\n                        \n                </Modal.Content>\n                <Modal.Content>\n                    <Modal.Description>\n                   <em> \n                       As a reminder, this site is not actually live. No actual charge has occurred to the provided Credit Card. The information was just passed through to Stripe CC Test environment\n                    </em>\n                    </Modal.Description>\n                        \n                </Modal.Content>\n\n\n                </>\n        )\n    }\n\n\n    const loading =()=>{\n        return (\n           \n                <Loader active={true}>Processing Order</Loader>\n            \n            \n            )\n\n\n    }\n\n    const failure =()=>{\n        return (\n            \n            <>\n                <Modal.Header> There was a Problem With the Purchase</Modal.Header>\n\n                <Modal.Content >\n                    <Modal.Description>\n                        There was an issue with placing your order. Please verify Credit Card infor and try again.\n                    </Modal.Description>\n                        \n                </Modal.Content>\n\n\n                </>\n\n\n        )\n    }\n\n\n    const contentRender =()=>{\n\n\n        if(props.status === \"loading\"){\n            return loading()\n        }\n        else if (props.status === \"success\"){\n            return success()\n        }\n        else return failure()\n    }\n\n    const closeHandler=()=>{\n\n        if(props.status===\"success\"){\n             history.push(\"/\")\n        }\n\n        if(props.status !==\"loading\"){\n            setOpen(false)\n        }\n        else if(longLoad){\n            props.setSuccess(\"failure\")\n            \n        }\n    }\n\n\n    return (\n        <Modal\n        closeIcon={props.status!==\"loading\" || longLoad}\n        open = {open}\n        onClose={closeHandler}\n\n        >\n\n            {contentRender()}\n        </Modal>\n\n\n    )\n}\n\n\n\n\nexport default PurchaseModal","/mnt/c/Users/Admin/dev/flatiron/labs/projects/kzmt/frontend/src/Container/Forms/LogIn.jsx",[],"/mnt/c/Users/Admin/dev/flatiron/labs/projects/kzmt/frontend/src/Component/PasswordRecovery/CompletePasswordRecoveryForm.jsx",[],{"ruleId":"287","replacedBy":"288"},{"ruleId":"289","replacedBy":"290"},{"ruleId":"287","replacedBy":"291"},{"ruleId":"289","replacedBy":"292"},{"ruleId":"293","severity":1,"message":"294","line":36,"column":5,"nodeType":"295","endLine":36,"endColumn":7,"suggestions":"296"},{"ruleId":"293","severity":1,"message":"297","line":46,"column":5,"nodeType":"295","endLine":46,"endColumn":20,"suggestions":"298"},{"ruleId":"293","severity":1,"message":"299","line":51,"column":5,"nodeType":"295","endLine":51,"endColumn":15,"suggestions":"300"},{"ruleId":"287","replacedBy":"301"},{"ruleId":"289","replacedBy":"302"},{"ruleId":"287","replacedBy":"303"},{"ruleId":"289","replacedBy":"304"},{"ruleId":"293","severity":1,"message":"305","line":23,"column":7,"nodeType":"295","endLine":23,"endColumn":9,"suggestions":"306"},{"ruleId":"287","replacedBy":"307"},{"ruleId":"289","replacedBy":"308"},{"ruleId":"287","replacedBy":"309"},{"ruleId":"289","replacedBy":"310"},{"ruleId":"293","severity":1,"message":"311","line":11,"column":7,"nodeType":"295","endLine":11,"endColumn":9,"suggestions":"312"},{"ruleId":"313","severity":1,"message":"314","line":1,"column":15,"nodeType":"315","messageId":"316