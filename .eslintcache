                 <div className=\"itemCardImageContainer\" >\n                     <Link to={itemURL} >\n                         <SmoothLoadingImage     \n                            className=\"itemImage\" \n                            src={props.extra[materialPicked].images[0]}\n                        />\n                    </Link>\n                </div>\n                <Card.Content>\n                \n                    <Header as={Link} to={itemURL} className=\"itemCardHeader\" size=\"tiny\" >\n                        {props.name}\n                    </Header>\n                    <Card.Description>\n                            <Button as={Link} to={itemURL} size=\"tiny\" className=\"shopping-cart-button\"> \n                                See More\n                            </Button>\n                    </Card.Description>\n                    \n                    <Card.Description>\n                    <div className=\"itemCardPrice\">{\"$\"+parseFloat(props.price).toFixed(2)}</div>\n                    </Card.Description>\n                    \n                </Card.Content>\n                <Card.Meta className=\"itemCardMaterials\">\n                    {metaDescriptions()}   \t\n                </Card.Meta>\n        </Card>  \n        \n\n       \n    )\n}\n\nexport const SpecialItemCard=(props)=>{\n\n    const [materialPicked,setMaterialPicked]= useState(props.materials[0].name)\n    \n    const changeMaterial =(e)=>{\n       e.preventDefault()\n        setMaterialPicked(e.target.dataset.name)\n    }\n\n    const itemURL= `/items/${props.slug}?material=${materialPicked}`\n\n\n\n    const metaDescriptions =()=>{  \n            return props.materials.map((element,index)=> {\n                                    if(index===4){\n                                        return (\n                                                <Link to={itemURL} title= \"see more styles\" >\n                                                        <div className =\"itemCardMaterialsButton\" style={{border: \"none\",fontWeight: \"bold\"}}  key={props.name+\" more button\"} >\n                                                            See More\n                                                        </div>\n                                                </Link>\n                                        )\n                                    }\n                                    else if (index <4){\n                                        return (\n                                                \n                                                    <div \n                                                    key={props.name+\" \"+element.name}\n                                                    title= {element.name}\n                                                    data-name={element.name}\n                                                    className =\"itemCardMaterialsButton\" \n                                                    style={\n                                                        {backgroundColor: materialColors[element.name],\n                                                        opacity: materialPicked=== element.name? \"50%\":null}} \n                                                        onClick={changeMaterial}  \n                                                        />\n                                        )\n                                        \n                                    }\n                            }\n            )\n        \n    }\n\n\n    return(\n        <>\n        \n            <Card className=\"specialItemCard\">\n\t\t\t\t\t <div className=\"specialItemCardImageContainer\"  >\n                         <Link to={itemURL} >\n                             <SmoothLoadingImage     \n                                className=\"specialItemImage\" \n                                src={props.extra[materialPicked].images[0]}\n                            />\n                        </Link>\n                    </div>\n\t\t\t\t\t<Card.Content>\n                    \n                        <Header as={Link} to={itemURL} className=\"itemCardHeader\" size=\"large\" >\n                            {props.name}\n                        </Header>\n                        <Card.Description>\n                            <Button as={Link} to={itemURL} size=\"small\" className=\"shopping-cart-button\"> \n                                See More\n                            </Button>\n                        </Card.Description>\n\t\t\t\t\t\t<Card.Description>\n                            <div className=\"itemCardPrice\">{\"$\"+parseFloat(props.price).toFixed(2)}</div>\n\t\t\t\t\t\t</Card.Description>\n                    </Card.Content>\n\t\t\t\t\t<Card.Meta className=\"itemCardMaterials\">\n                        {metaDescriptions()}   \t\n\t\t\t\t\t</Card.Meta>\n            </Card>  \n            \n\n            </>\n    )\n}\n\n\n\nexport default ItemCard\n\n\n// <Card className=\"itemCard\" title={props.name} fluid style={{background:\"red\"}}>\n//                 <Image as={Link} to={\"items/\"+props.slug} size=\"medium\" src={props.images[0].image_url} />\n//             <Grid centered>\n//                 <Grid.Row  >\n            \n//                     {metaDescriptions()}\n                    \n//                 </Grid.Row>\n//             </Grid>\n//             <Grid>\n//                 <Grid.Row>\n//                     <Grid.Column width=\"10\" floated=\"left\">\n//                         <Card.Content className=\"itemCardHeader\">{props.name}</Card.Content>\n//                     </Grid.Column>\n                  \n//                      <Grid.Column width=\"6\" floated=\"right\">\n//                     <Card.Content textAlign=\"right\" verticalAlign=\"top\" className=\"itemCardPrice\"> {\"$\"+parseFloat(props.price).toFixed(2)} </Card.Content>\n//             </Grid.Column>\n\n//                 </Grid.Row>\n//             </Grid>\n            \n            \n\n//         </Card>","/mnt/c/Users/Admin/dev/flatiron/labs/projects/kzmt/frontend/src/Component/ShoppingCart/ShoppingCartItem.jsx",[],"/mnt/c/Users/Admin/dev/flatiron/labs/projects/kzmt/frontend/src/Container/MenuBar.jsx",[],"/mnt/c/Users/Admin/dev/flatiron/labs/projects/kzmt/frontend/src/Component/LoadingScreen.jsx",[],"/mnt/c/Users/Admin/dev/flatiron/labs/projects/kzmt/frontend/src/Container/Checkout.jsx",["265","266"],"import React,{useState,useEffect,useCallback} from 'react'\nimport {Link} from 'react-router-dom'\nimport {Grid,Container, Button, Accordion, Icon} from 'semantic-ui-react'\nimport {Elements, StripeProvider} from 'react-stripe-elements'\n\nimport CartContextConsumer from '../Context/CartContextConsumer'\nimport UserContextConsumer from '../Context/UserContextConsumer'\n\nimport debounce from 'lodash.debounce'\n\nimport CartComponents from '../Component/Checkout/Cart'\nimport OrderSummary from '../Component/Checkout/OrderSummary'\nimport PurchaseModal from '../Component/Checkout/PurchaseModal'\nimport LogInModal from '../Component/Checkout/LogInModal'\n\nimport CCPaymentForm from '../Component/Checkout/CCPaymentForm'\nimport ShippingAddress from './Forms/ShippingAddress'\nimport BillingAddress from './Forms/BillingAddress'\n\n\n\n\n\n\n\nconst CheckoutContainer = (props)=>{\n\n    useEffect(()=>{\n        document.title= \"Checkout -\"+ process.env.REACT_APP_TITLE\n        checkout() },[])\n\n    const [cartFinalized, setCartFinalized] = useState(true)   \n    const [activeAccordion,setActiveAccordion] = useState(\"cart\") \n    const [token,setToken] = useState()\n    const [ address, setAddress]= useState({shipping:{firstName: \"\", lastName: \"\", street:\"\",city: \"\", state: \"\", zip: \"\"}, billing: {sameShipping: false, firstName: \"\", lastName: \"\",street:\"\",city: \"\", state: \"\", zip: \"\"} }) \n    const [success,setSuccess] = useState()\n    const [unAuthorized,setUnAuthorized] = useState(!props.user)\n\n    const checkout=()=>{\n        const body ={orders: {items: props.cart}}\n        const options = {\n            method: \"POST\",\n            headers: {\n                \"Content-Type\": \"application/json\",\n                \"Accept\": \"application/json\"\n              },\n              credentials: 'include',\n              body: JSON.stringify(body)\n            }\n        fetch(process.env.REACT_APP_API_URL + \"checkout\" , options)\n        .then(response => {if(!response.ok){\n            throw new Error(response.status)\n         }\n          else return response.json()})\n        .then(itemsObj =>  createItems(itemsObj))\n        .catch(()=> setUnAuthorized(true))\n    }\n\n    const purchase = ()=>{\n        setSuccess(\"loading\")\n        const body = {orders:{\n            token: token,\n            shipping: address.shipping\n            }\n        }\n\n        const options = {\n            method: \"POST\",\n            headers: {\n                \"Content-Type\": \"application/json\",\n                \"Accept\": \"application/json\"\n              },\n              credentials: 'include',\n              body: JSON.stringify(body)\n            }\n\n            fetch(process.env.REACT_APP_API_URL + \"checkout/purchase\" , options)\n            .then(response => {if(!response.ok){\n                throw new Error(response.status)\n             }\n                else return response.json()\n            })\n             .then(messageResponse => {\n                purchaseSuccess() \n             })\n             .catch(()=> setSuccess(\"failure\"))\n\n\n\n\n    }\n\n    const purchaseSuccess = (itemsObject)=>{\n        props.changeCart({instruction: \"delete_all\"})\n        setSuccess(\"success\")\n        setAddress({shipping:{firstName: \"\", lastName: \"\", street:\"\",city: \"\", state: \"\", zip: \"\"}, billing: {sameShipping: false, firstName: \"\", lastName: \"\",street:\"\",city: \"\", state: \"\", zip: \"\"} })\n        setToken()\n\n\n\n\n    }\n\n    const debouncedFunction = useCallback(debounce(checkout,2000),[cartFinalized])\n\n    useEffect(()=>{\n        if(cartFinalized){\n            debouncedFunction()\n        }\n\n        return debouncedFunction.cancel\n          \n    },[cartFinalized,debouncedFunction]) \n\n\n    const createItems = (items)=>{\n        const cart=  items.map(itemElement=> {\n            const material = itemElement.material\n          return({\n            name: itemElement.item.name,\n            slug: itemElement.item.slug,\n            material: material,\n            image: itemElement.item.extra[material].images[0],\n            price: itemElement.item.price,\n            id: itemElement.item.extra[material].id,\n            quantity: itemElement.quantity\n        })\n        })\n\n    props.changeCart({instruction: \"replace_all\", replacementCart: cart})  \n    }\n\n    return(\n        \n        <Container className=\"page-container-grid\">\n            <Grid  verticalAlign=\"top\" className=\"page-container-grid\" centered={true} stackable={true}>\n                <Grid.Row>\n                    <h1 >Checkout</h1>\n                </Grid.Row>\n                <Grid.Row columns=\"4\" >\n                    <Grid.Column width=\"12\">\n                        <Accordion fluid styled className=\"checkout-accordion\"> \n                            <Accordion.Title active={activeAccordion===\"cart\"} >\n                               <h2>\n                                   <Icon name=\"dropdown\"/>\n                                   Cart\n                                </h2>\n                            </Accordion.Title>\n                            <Accordion.Content active={activeAccordion===\"cart\"}>\n                                <CartComponents cart={props.cart}   cartFinalized={cartFinalized} setCartFinalized={setCartFinalized} changeCart={props.changeCart} setActiveAccordion={setActiveAccordion}/>\n                            </Accordion.Content>\n                            <Accordion.Title active={activeAccordion===\"shipping\"} >\n                                <h2>\n                                    <Icon name=\"dropdown\"/>\n                                    Shipping\n                                </h2>\n                            </Accordion.Title>\n                            <Accordion.Content active={activeAccordion===\"shipping\"}>\n                                <ShippingAddress address={address} setAddress={setAddress} setActiveAccordion={setActiveAccordion}/>\n                            </Accordion.Content>\n                            <Accordion.Title active={activeAccordion===\"billing\"} >\n                                <h2>\n                                    <Icon name=\"dropdown\"/>\n                                    Billing\n                                </h2>\n                            </Accordion.Title>\n                            <Accordion.Content active={activeAccordion===\"billing\"}>\n                                <BillingAddress address={address} setAddress={setAddress} setActiveAccordion={setActiveAccordion}/>\n                            </Accordion.Content>\n                            <Accordion.Title active={activeAccordion===\"CC\"}  >\n                                <h2>\n                                    <Icon name=\"dropdown\"/>\n                                    Credit Card Information\n                                </h2>\n                            </Accordion.Title>\n                            <Accordion.Content active={activeAccordion===\"CC\"}>\n                                    <StripeProvider apiKey={process.env.REACT_APP_STRIPE_KEY} >\n                                        <Elements \n                                            >\n\n                                            <CCPaymentForm  setActiveAccordion={setActiveAccordion} setToken={setToken}/>\n                                        </Elements>\n                                    </StripeProvider>\n\n\n                            </Accordion.Content>\n\n                            <Accordion.Content active={activeAccordion===\"complete\" && props.user}>\n                            <Button.Group fluid>\n                                <Button id=\"checkout-cart-finalize-button\"   onClick={()=>setActiveAccordion(\"cart\")}>\n                                    Restart\n                                </Button>\n                                {props.cart.length >0?\n                                        <>\n                                                    <Button.Or />\n                                        \n                                                <Button className=\"shopping-cart-button\" onClick={()=>purchase()}>\n                                                    Confirm Payment\n                                                </Button>\n                                        </>\n                                : null}\n                            </Button.Group>\n                            </Accordion.Content>\n\n                            <Accordion.Content active={!props.user}>\n                            <Button.Group fluid>\n                                \n                                <Button as={Link} to=\"/LogIn\" className=\"shopping-cart-button\">\n                                    Need to Login to Purchase\n                                </Button>\n                            </Button.Group>\n                            </Accordion.Content>\n                            \n                    </Accordion>\n                    </Grid.Column>\n                    <Grid.Column width=\"4\" id=\"order-summary\">\n                        <OrderSummary cart={props.cart} />\n                    </Grid.Column>\n                </Grid.Row>\n                \n                \n            </Grid>\n            <PurchaseModal status={success} setSuccess={setSuccess} user={props.user} />\n            <LogInModal status={unAuthorized}  />\n        </Container>\n\n    )\n}\n\nexport default CartContextConsumer(UserContextConsumer (CheckoutContainer))\n","/mnt/c/Users/Admin/dev/flatiron/labs/projects/kzmt/frontend/src/Container/Inquiry/Inquiry.jsx",[],"/mnt/c/Users/Admin/dev/flatiron/labs/projects/kzmt/frontend/src/Container/HomePage/HomepageCategories.jsx",[],"/mnt/c/Users/Admin/dev/flatiron/labs/projects/kzmt/frontend/src/Component/HomePage/HomePagePortfolioBanner.jsx",[],"/mnt/c/Users/Admin/dev/flatiron/labs/projects/kzmt/frontend/src/Component/Items/ItemsImage.jsx",[],"/mnt/c/Users/Admin/dev/flatiron/labs/projects/kzmt/frontend/src/Component/Inquiry/Repair.jsx",[],"/mnt/c/Users/Admin/dev/flatiron/labs/projects/kzmt/frontend/src/Component/Menu/DropdownComponent.jsx",[],"/mnt/c/Users/Admin/dev/flatiron/labs/projects/kzmt/frontend/src/Component/Checkout/CCPaymentForm.jsx",[],"/mnt/c/Users/Admin/dev/flatiron/labs/projects/kzmt/frontend/src/Component/Logos/404.js",["267"],"/mnt/c/Users/Admin/dev/flatiron/labs/projects/kzmt/frontend/src/Component/Checkout/PurchaseModal.jsx",[],"/mnt/c/Users/Admin/dev/flatiron/labs/projects/kzmt/frontend/src/Container/Forms/LogIn.jsx",[],{"ruleId":"268","replacedBy":"269"},{"ruleId":"270","replacedBy":"271"},{"ruleId":"268","replacedBy":"272"},{"ruleId":"270","replacedBy":"273"},{"ruleId":"274","severity":1,"message":"275","line":33,"column":5,"nodeType":"276","endLine":33,"endColumn":7,"suggestions":"277"},{"ruleId":"274","severity":1,"message":"278","line":43,"column":5,"nodeType":"276","endLine":43,"endColumn":20,"suggestions":"279"},{"ruleId":"268","replacedBy":"280"},{"ruleId":"270","replacedBy":"281"},{"ruleId":"274","severity":1,"message":"282","line":23,"column":7,"nodeType":"276","endLine":23,"endColumn":9,"suggestions":"283"},{"ruleId":"268","replacedBy":"284"},{"ruleId":"270","replacedBy":"285"},{"ruleId":"286","severity":1,"message":"287","line":1,"column":15,"nodeType":"288","messageId":"289","endLine":1,"endColumn":24